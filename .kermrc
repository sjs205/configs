; See http://www.columbia.edu/kermit/k95manual/setterm.html for notes. 
; Line properties

;SET MODEM TYPE			none		; Direct connection
SET LINE 			/dev/ttyS0

;Line speed
;set speed			2400		
;set speed			4800		
;set speed			9600		
;set speed			14400		
;set speed			19200		
;set speed			38400		
;set speed			57600		
SET SPEED			115200		
;set TERMINAL CR-DISPLAY CRLF
SET CARRIER-WATCH		OFF		; No carried expected
;SET HANDSHAKE			NONE		; NO HANDSHAKING
;SET FLOW-CONTROL		NONE		; NO FLOW CONTROL
;SET FLOW-CONTROL		XON/OFF		; SOFTWARE FLOW CONTROL
SET FLOW-CONTROL		RTS/CTS		; HARDWARE FLOW CONTROL
;SET STOP-BITS			0		; NO STOP-BITS
;SET STOP-BITS			1		; 1 STOP-BITS
SET PARITY NONE
SET TERMINAL ECHO		REMOTE
;SET TERMINAL ECHO	    LOCAL
;SET DEBUG 			SESSION
;COMMUNICATION PROPERTIES:

ROBUST						; MOST ROBUST TRANSFER SETTING
SET RECEIVE PACKET-LENGTH	1000		; MAX REMOTE SYSTEM SHOULD USE
SET SEND PACKET-LENGTH		1000		; MAX LOCAL SYSTEM SHOULD USE
SET WINDOW			10		; NUM PACKETS TO SEND UNTIL ACK

;FILE TRANSFER PROPERTIES

SET FILE TYPE			BINARY		; ALL FILES TRANSFERED BINARY
SET FILE NAMES			LITERAL		; DON'T MOD FILENAMES DURING TX
